@{
    ViewBag.Title = "RETURN URL FROM VNPAY";
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>@ViewBag.Title</title>
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
</head>
<body>
    <div class="container">
        <div class="header clearfix">
            <h3 class="text-muted">Kết quả thanh toán</h3>
        </div>
        <div class="table-responsive">
            <p>Message: @ViewBag.Message</p>
            <p>Terminal ID: @ViewBag.TerminalID</p>
            <p>Transaction Reference: @ViewBag.TxnRef</p>
            <p>VnPay Transaction Number: @ViewBag.VnPayTranNo</p>
            <p>Amount: @ViewBag.Amount</p>
            <p>Bank Code: @ViewBag.BankCode</p>
        </div>
    </div>
</body>
</html>
